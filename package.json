{
	"name": "angularjsfileupgrade",
	"publisher": "TomTec",
	"displayName": "TomTecAngularjsFileUpgrade",
	"description": "Upgrade file naming convention from angularjs to angular",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ttFileUpgrade.upgradeFileName"
	],
	"repository": {
		"type": "git",
    "url": "https://github.com/andy-maca/AngularjsFilenameUpgrade.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ttFileUpgrade.upgradeFileName",
				"title": "TomTec angular - upgrade current active file name"
			},
			{
				"command": "ttFileUpgrade.explorer.upgradeFileName",
				"title": "TomTec angular - upgrade selected files' name"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "ttFileUpgrade.explorer.upgradeFileName"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^7.0.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0"
	},
	"dependencies": {}
}
